
// Format:
// {
//     name: "Command Name" (string),
// 
//     description: "What does the command do?" (string),
// 
//     minLevel: "Which level people can use the command?" (integer),
// 
//     alias: [ "alias 1", "alias 2" ... no limit ],
//
//     acceptArgs: What arguments are acceptable? (array of strings)
//              [ options: 
//                         "target" - Mention a person
//                         "slot" - if the player is currently in server
//                         "b3id" - !id of player
//                         "guid" - !xuid of player
//                         "visible2all" - should it be visible to everyone
//                         "others" - for other type of commands like !mask and !greeting
//              ],
//               Example: acceptArgs: [ "target/b3id/guid", "visible2all" ],
//               --> will make it so command accepts one of target/b3id/guid and visible2all as arguments                        
//                        acceptArgs: [], --> accept no arguments for command
//
//     // if acceptableArgs includes "others"
//     // type of this arg also has to be mentioned, options are: "number" | "string" | "array"
//     otherArgs: { "1st other arg": true/false (required or optional) }
//      
//     
//     // How should the command be called?
//     type: "slash" | "prefix" | "both" (string, enter just one of the options)
//
// },

